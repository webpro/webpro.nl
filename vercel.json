{
  "framework": "astro",
  "outputDirectory": ".vercel/output/static",
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' 'unsafe-inline' smplnltcs.webpro.nl"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/scraps/using-path-aliases-with-tsc",
      "destination": "/articles/using-subpath-imports-and-path-aliases",
      "permanent": true
    }
  ]
}
