{
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' 'unsafe-inline' smplnltcs.webpro.nl"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/articles/introduction-to-the-terminal-for-developers",
      "destination": "/articles/introducing-the-terminal-to-developers",
      "permanent": true
    },
    {
      "source": "/articles/how-to-build-a-great-theme-switch-toggle",
      "destination": "/articles/how-to-build-a-great-theme-toggle-switch",
      "permanent": true
    }
  ]
}
