{
  "retryOn429": true,
  "ignorePatterns": [
    {
      "pattern": "^/blog$"
    },
    {
      "pattern": "LarsKappert-CV.pdf$"
    },
    {
      "pattern": "linkedin.com"
    },
    {
      "pattern": "^mailto:"
    }
